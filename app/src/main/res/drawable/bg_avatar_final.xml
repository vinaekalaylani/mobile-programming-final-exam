<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Lapisan 1: Bingkai kuning paling luar (warna hardcode) -->
    <item>
        <shape android:shape="oval">
            <solid android:color="#FFC107" />
        </shape>
    </item>

    <!-- Lapisan 2: Latar belakang biru, menjorok 4dp (warna hardcode) -->
    <item android:left="4dp" android:top="4dp" android:right="4dp" android:bottom="4dp">
        <shape android:shape="oval">
            <solid android:color="#1A237E" />
        </shape>
    </item>

    <!-- Lapisan 3: Ikon putih, menjorok 20dp (warna hardcode) -->
    <item android:left="20dp" android:top="20dp" android:right="20dp" android:bottom="20dp">
        <bitmap
            android:src="@drawable/ic_account_circle"
            android:tint="#FFFFFFFF" />
    </item>

</layer-list>
